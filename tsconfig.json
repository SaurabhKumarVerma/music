{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "module": "es2015",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@music/*": ["src/*"],
      "@components/*": ["src/component/*"],
      "@base/*":["src/base/*"],
      "@hooks/*": ["src/hook/*"],
      "@services/*":["src/service/*"],
      "@themes/*":["src/theme/*"],
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@screens/*":["src/screen/*"],
      "@navigations-stack/*": ["src/navigation/StackNavigation/*"],
      "@store/*": ["src/store/*"],
      "@navigationTypes/*":["src/navigation/NavigationTypes/types"],
      "@navigation-taps/*": ["src/navigation/BottomNavigation/*"],
      "@navigations-drawer/*": ["src/navigation/DrawerNavigation/*"],
      "@constant/*":["src/constant/*"],
    }
  },
  "extends": "expo/tsconfig.base",
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "exclude": ["node_modules"],
  "include": [
    // "next-env.d.ts",
    // "additional.d.ts",
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.tsx",
    "./App.tsx",
    "./src/**/*",
    "src/**/*",
    "src/**/*.*",
  ],
}
